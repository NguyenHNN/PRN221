<Window x:Class="DAY3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DAY3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Name="btnTop5Tren" Content="Top 5 thí sinh có điểm Toán từ trên xuống" Width="300" Height="40" Margin="5" Click="btnTop5Tren_Click"/>
            <Button Name="btnTop5Duoi" Content="Top 5 thí sinh có điểm Toán từ dưới lên" Width="300" Height="40" Margin="5" Click="btnTop5Duoi_Click"/>
            <Button Name="btnThukhoaA" Content="Thí sinh là Thủ khoa khối A" Width="300" Height="40" Margin="5" Click="btnThukhoaA_Click" />
            <Button Name="btnCount" Content="Số lượng thí sinh tham gia thi từng môn" Width="300" Height="40" Margin="5" Click="btnCount_mon_Click" />
            <Button Name="btnCount2" Content="Số lượng thí sinh tham gia thi Khoa Học Tự Nhiên và Khoa Học Xã Hội" Width="400" Height="40" Margin="5" Click="btnCount_khtn_khxh_Click" />
            <TextBox Name="txtResult" Height="200" Width="400" />
        </StackPanel>
    </Grid>
</Window>
